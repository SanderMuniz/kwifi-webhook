{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "lib": ["es2020"],
    "allowJs": true,
    "checkJs": false,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": ".vercel/output/src",
    "rootDir": ".",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "api/**/*.ts",
    "config/**/*.ts",
    "services/**/*.ts",
    "utils/**/*.ts",
    "**/*.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}